{
    "env": {
        "es2021": true,
        "node": true,
        "browser": true,
        "jest": true
    },
    "extends": ["standard", "prettier"],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
